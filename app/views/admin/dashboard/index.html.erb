

<h3>List of Users</h3>
  <table border="1" cellpadding="10">
    <thead>
      <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
        <th>Created At</th>
        <th>Updated At</th>
        <th>Is Admin?</th>
        <th>Approved?</th>
        <th>Score</th>
      </tr>
    </thead>

    <tbody>
      <% @users.each do |user| %>
        <tr>
          <td><%= user.first_name %></td>
          <td><%= user.last_name %></td>
          <td><%= user.email %></td>
          <td><%= user.created_at %></td>
          <td><%= user.updated_at %></td>
          <td><%= user.admin? %></td>
          <td><%= user.approved_student? %></td>
          <td><%= user.score %></td>
          
        </tr>
      <% end %>
    </tbody>